<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/settings_nav_graph"
    app:startDestination="@id/mainSettingsFragment">

    <fragment
        android:id="@+id/mainSettingsFragment"
        android:name="com.example.clothingstoreapp.FragmentLayout.MainSettingsFragment"
        android:label="fragment_main_settings"
        tools:layout="@layout/fragment_main_settings" >
        <action
            android:id="@+id/action_mainSettingsFragment_to_myAddressFragment"
            app:destination="@id/myAddressFragment" />
    </fragment>
    <fragment
        android:id="@+id/myAddressFragment"
        android:name="com.example.clothingstoreapp.FragmentLayout.MyAddressFragment"
        android:label="fragment_my_address"
        tools:layout="@layout/fragment_my_address" >
        <action
            android:id="@+id/action_myAddressFragment_to_add_AddressFragment"
            app:destination="@id/add_AddressFragment" />
    </fragment>
    <fragment
        android:id="@+id/add_AddressFragment"
        android:name="com.example.clothingstoreapp.FragmentLayout.Add_AddressFragment"
        android:label="fragment_add__address"
        tools:layout="@layout/fragment_add__address" >
        <action
            android:id="@+id/action_add_AddressFragment_to_selectNewAddressFragment"
            app:destination="@id/selectNewAddressFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectNewAddressFragment"
        android:name="com.example.clothingstoreapp.FragmentLayout.SelectNewAddressFragment"
        android:label="fragment_select_new_address"
        tools:layout="@layout/fragment_select_new_address" />
</navigation>